#include <iostream>
#include <cmath>

int main() {
    int x, y;
    std::cout << "Enter the value of 'x': ";
    std::cin >> x;
    std::cout << "Enter the value of 'y': ";
    std::cin >> y;

    int result = std::pow(x, y);

    std::cout << x << " raised to the power of " << y << " is: " << result << std::endl;

    return 0;
}
