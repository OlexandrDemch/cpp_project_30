#include <iostream>

int main() {
    int a;
    std::cout << "Enter the value of 'a' (1 <= a <= 20): ";
    std::cin >> a;

    if (a < 1 || a > 20) {
        std::cerr << "Invalid input for 'a'. Please enter a value between 1 and 20." << std::endl;
        return 1;
    }

    long long product = 1;
    for (int i = a; i <= 20; ++i) {
        product *= i;
    }

    std::cout << "Product from " << a << " to 20 is: " << product << std::endl;

    return 0;
}
